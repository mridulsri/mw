@model AddReport

<form asp-controller="Home" asp-action="SubmitReport" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" class="">
    <input type="hidden" asp-for="ProjectFeedId" />
    <div class="row">
        <div class="form-group">
            <label asp-for="Report" class="control-label"></label>
            <textarea asp-for="Report"></textarea>
            <span asp-validation-for="Report" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class="control-label">Upload Invoice</label>
            <input type="file" name="file" id="FileInput">
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-block btn-primary">Submit</button>
        </div>
    </div>

</form>
